{
	"name": "odysseus",
	"version": "0.0.1",
	"description": "Projeto Backend do sistema Odyssey da Escola SistÃªmica",
	"author": "Faculdade Senai Joinville",
	"private": true,
	"license": "MIT",
	"scripts": {
		"refresh:project": "yarn cache clean && rm -rf node_modules && yarn install",
		"test": "jest",
		"win:cov": "start ./coverage/lcov-report/index.html",
		"lin:cov": "xdg-open ./coverage/lcov-report/index.html",
		"mac:cov": "open ./coverage/lcov-report/index.html",
		"prebuild": "rimraf dist",
		"start": "nest start",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"build": "nest build",
		"dev": "nest start --watch",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
		"ormconfig:generate": "(rm -rf ormconfig.json) && ts-node -r tsconfig-paths/register ./scripts/ormconfig.script.ts",
		"mongo:script:new": "ts-node -r tsconfig-paths/register ./scripts/dml/new_mongo_dml.script.ts",
		"mongo:script:run": "ts-node -r tsconfig-paths/register ./scripts/dml/run_mongo_dml.script.ts",
		"mysql:script:new": "ts-node -r tsconfig-paths/register ./scripts/dml/new_mysql_dml.script.ts",
		"mysql:script:run": "ts-node -r tsconfig-paths/register ./scripts/dml/run_mysql_dml.script.ts",
		"mysql:migration:new": "yarn typeorm migration:create -c mysqlConnection -n",
		"mysql:migration:run": "yarn typeorm migration:run -c mysqlConnection",
		"db:magic": "yarn ormconfig:generate && yarn migration:run"
	},
	"dependencies": {
		"@nestjs/common": "^7.6.13",
		"@nestjs/config": "^0.6.3",
		"@nestjs/core": "^7.6.13",
		"@nestjs/jwt": "^7.2.0",
		"@nestjs/passport": "^7.1.5",
		"@nestjs/platform-express": "^7.6.13",
		"@nestjs/swagger": "^4.8.0",
		"@nestjs/typeorm": "^7.1.5",
		"bcrypt": "^5.0.1",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongodb": "^3.6.4",
		"mysql2": "^2.2.5",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.0.0",
		"swagger-ui-express": "^4.1.6",
		"typeorm": "^0.2.31",
		"webpack": "*",
		"yup": "^0.32.9"
	},
	"devDependencies": {
		"@nestjs/cli": "^7.5.6",
		"@nestjs/schematics": "^7.2.7",
		"@nestjs/testing": "^7.6.17",
		"@types/bcrypt": "^3.0.0",
		"@types/express": "^4.17.11",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.31",
		"@types/passport-jwt": "^3.0.5",
		"@types/supertest": "^2.0.10",
		"@types/yup": "^0.29.11",
		"@typescript-eslint/eslint-plugin": "^4.15.2",
		"@typescript-eslint/parser": "^4.15.2",
		"eslint": "^7.21.0",
		"jest": "^26.6.3",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.2",
		"ts-loader": "^8.0.17",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.1.5"
	}
}
